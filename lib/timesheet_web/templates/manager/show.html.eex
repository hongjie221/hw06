<h1>Show Manager</h1>

<ul>

  <li>
    <strong>Id:</strong>
    <%= @manager.id %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @manager.name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @manager.email %>
