
<% {sheet_id, sheet_status} = @sheet_detail%>
<% [approved] = sheet_status %>
<%= if approved do %>
  <span>Sheet  Status: Approved</span>
<% else %>
  <span>Sheet Status: Not Approved</span>
<% end %>
<% tasks = Timesheet.Tasks.get_task_by_sheet_id(sheet_id) %>
 


<%= for {jobcode, hour, note} <- tasks do %>
<tbody>
  <tr>
    <td><%= jobcode %></td>
    <td><%= hour %></td>
    <td><%= note %></td>
  </tr>
</tbody>
<% end %>

